data State = Filled | Blank -- состояние ячейки

data Cell = (State, State) -- ячейка (текущее, правильное)

data Line = [Cell] -- строка или столбец

data Field = [Cell] -- игровое поле

makeHorline :: Field -> Int -> Line -- взять столбец

makeVerline :: Field -> Int -> Line -- взять строку

makeNum :: Line -> [Int] -- сделать из строки / столбца массив цифорок

checkfield :: Field -> Bool -- проверить поле: все текущие == правильные для победы

changecell :: Field -> Int -> Int -> Field -- воздействие на поле

